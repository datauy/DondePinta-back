<p id="notice"><%= notice %></p>

<h1>Listing Venues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Venue type</th>
      <th>Department</th>
      <th>City</th>
      <th>Address</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Web</th>
      <th>Foursquare</th>
      <th>Lat</th>
      <th>Lng</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.name %></td>
        <td><%= venue.venue_type %></td>
        <td><%= venue.department %></td>
        <td><%= venue.city %></td>
        <td><%= venue.address %></td>
        <td><%= venue.phone %></td>
        <td><%= venue.email %></td>
        <td><%= venue.web %></td>
        <td><%= venue.foursquare_id %></td>
        <td><%= venue.lat %></td>
        <td><%= venue.lng %></td>
        <td><%= link_to 'Show', venue %></td>
        <td><%= link_to 'Edit', edit_venue_path(venue) %></td>
        <td><%= link_to 'Destroy', venue, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Venue', new_venue_path %>
